curl -X POST https://notify.twilio.com/v1/Credentials \
--data-urlencode "Type=apn" \
--data-urlencode "Certificate=cert.pem_content" \
--data-urlencode "FriendlyName=MyAPNCredential" \
--data-urlencode "PrivateKey=key.pem_content" \
--data-urlencode "Sandbox=True" \
-u ACaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa:your_auth_token